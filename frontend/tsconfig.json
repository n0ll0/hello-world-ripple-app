{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"allowImportingTsExtensions": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"moduleResolution": "bundler",
		"jsx": "preserve",
		"jsxImportSource": "ripple",
		"noEmit": true,
		"isolatedModules": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	}
}
